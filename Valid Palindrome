class Solution:
    def isPalindrome(self, s: str) -> bool:
        string=''
        for a in s.lower():
            if a.isalnum():
                string+=a
        l=0
        r=len(string)-1
        while l < r:
            if string[l] != string[r]:
                return False
            l += 1
            r -= 1
        else:
            return True
        


        
